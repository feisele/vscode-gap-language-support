{
    "name": "gap-language-support",
    "displayName": "gap-language-support",
    "description": "Provides syntax highlighting and basic code completion for GAP (www.gap-system.org)",
    "publisher": "feisele86",
    "repository" :
         { "type" : "git", "url" : "https://github.com/feisele/gap-language-support.git" },
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "gap",
                "aliases": [
                    "GAP",
                    "gap"
                ],
                "extensions": [
                    ".g",
                    ".gi",
                    ".gd",
                    "..gaprc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gap",
                "scopeName": "source.gap",
                "path": "./syntaxes/gap.tmLanguage"
            }
        ]
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.32.0",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "eslint": "^7.1.0",
        "triejs": "^0.1.5",
        "typescript": "^4.0.2",
        "memoizee": "^0.4.15",
        "path": "^0.12.7",
        "vscode-cache": "^0.3.0"
    }
}
